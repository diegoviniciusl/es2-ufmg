{
    "env": {
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "no-console": 0,
        "max-len": "off",
        "no-param-reassign": "off"
    },
    "overrides": [
        {
            "files": [
                "__test__/**/*"
            ],
            "env": {
                "jest": true
            }
        }
    ]
}